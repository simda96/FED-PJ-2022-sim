			function SetTopMatchResultInfo(){
				// data-id = ('1차전':'W14', '2차전':'W30', '3차전':'W46');
				var $banner = $("#match-kor-banner");
				var match_cd = $banner.data("id");
   				var mat = Worldcup2022.matches_cd[match_cd];
				//console.log('SetTopMatchResultInfo mat', match_cd, mat);
				if(mat && mat.complete){
					var str_html = make_group_league_html(match_cd, true, true);
					console.log('SetTopMatchResultInfo html', str_html);
					$banner.find(".score-result").html(str_html.score);
					if(str_html.is_pk){
						$banner.addClass("result result-pk");
						$banner.find(".match-wrap").addClass("result result-pk");
					} else {
						$banner.addClass("result");
						$banner.find(".match-wrap").addClass("result");
					}
				}
			}

			Worldcup2022.ready_callback = function(){
				SetTopMatchResultInfo();
			};
		</script>
